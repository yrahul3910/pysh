{
    "scopeName": "source.pysh",
    "fileTypes": ["pysh"],
    "injectionSelector": "L:source.python",
    "patterns": [
        {
            "begin": "(\\w[a-zA-Z0-9.]+)?`",
            "end": "`",
            "patterns": [{
                "include": "#shell-syntax"
            }]
        }
    ],
    "repository": {
        "shell-syntax": {
            "match": ".*",
            "name": "string.shell"
        }
    }
}